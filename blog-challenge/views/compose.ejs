<%- include('partials/header') -%>

<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input class="form-control" type="text" name="postTitle" value="">
  </div>
  <div class="form-group">
    <label for="postBody">Post</label>
    <textarea class="form-control" name="postBody" rows="5" cols="80"></textarea>
  </div>
  <div class="form-group">
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
  </div>

</form>

<%- include('partials/footer') -%>
